{
  "name": "clockdown",
  "version": "1.0.0",
  "description": "Simple game timer",
  "scripts": {
    "test": "elm-test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "dev": "parcel src/index.html",
    "build": "rm -rf dist && parcel build src/index.html --dist-dir dist --public-url ./",
    "format": "elm-format --yes src/ tests/",
    "review": "elm-review",
    "deps": "elm-json"
  },
  "author": "adamnfish",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-elm": "^2.12.0",
    "@playwright/test": "^1.40.0",
    "elm": "^0.19.1-6",
    "elm-format": "^0.8.7",
    "elm-json": "^0.2.13",
    "elm-review": "^2.13.4",
    "elm-test": "^0.19.1-revision12",
    "parcel": "^2.12.0",
    "playwright": "^1.40.0"
  }
}
